
http
  port 8080

schema example/base
  attribute user/name one string "This is a name"

schema example/schema
  require example/base

api example/v1
  require example/schema
  get "/user"       get-all-users
  get "/user/:name" [require-authentication get-user sample.vase.v1/users-page]

interceptor authenticated?
  enter (fn [ctx] (assoc ctx :authenticated? false))

query get-user
  q [:find (pull ?e [*]) :where [?e :user/name ?name]]
  params [name]
